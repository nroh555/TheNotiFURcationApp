{
  "manifest_version": 3,
  "name": "The Notifurcation App",
  "description": "The Notifurcation App",
  "version": "1.0.0",
  "icons": { "128": "icon-128.png" },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon-128.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "notifications", "alarms"],
  "host_permissions": ["<all_urls>"]
}
